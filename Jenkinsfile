pipeline {
  agent any

  stages {
    stage('Checkout') {
      steps {
        echo 'Fazendo checkout'
      }
    }

    stage('Construcao') {
      steps {
        echo 'Fazendo build'
      }
    }

    stage('Entrega') {
      steps {
        echo 'Fazendo delivery'
      }
    }

    stage('Cleanup') {
      steps {
        echo 'Fazendo cleanup'
      }
    }
  }
}